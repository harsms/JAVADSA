package com.patterns.matching;

public class Number1 {

	public static void num(int n) {
		
		for(int i =1 ; i <= n ; i ++) {
			
			
			for(int j =1 ; j<= i ; j++) {
				
				System.out.print(j);
			}
			
			System.out.println("\n");
			
		}
		
	}
	
	
	public static void main(String[] args) {
		num(5);

	}

}
